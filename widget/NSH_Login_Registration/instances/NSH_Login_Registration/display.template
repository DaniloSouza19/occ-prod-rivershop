<div class="login_registration" data-bind="onRender: rendered">
   <div class="containerss">
      <div class="limiter">
         <!---------------- Inicio LOGIN ----------------------->
            <div class="container">
               <div class="login-banner col-xs-12 col-lg-8 col-md-12 col-sm-12"
                  data-bind="data: $widgetViewModel._mediaImage">
                  <!-- <img class="m-t-40"  src="file/general/997 x 550px.png" width="100%" alt=""> -->
                  <img class="m-t-40" data-bind="attr: {src: $widgetViewModel._mediaImage.src()}" width="100%" />
               </div>
               <div class="login-register col-xs-12 col-lg-4 col-md-12 col-sm-12">
                  <div id="login" class="wrap-login100 ">
                     <div class="row">
                        <div>
                           <div class="mdl-cnt__box log-box col-xs-12">
                              <span class="login100-form-title p-b-15">
                                 <img src="file/general/logo-login.news_black.png" width="100%" alt="River Shop"
                                    title="River Shop">
                              </span>
                              <div>
                                 <div class="login100-form validate-form ">
                                    <div>
                                       <div>
