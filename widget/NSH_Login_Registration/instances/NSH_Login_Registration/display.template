                                                         <button class="login100-form-btn col-xs-12 mt-20 m-t-20"
                                                            id="CC-updatePasswordCancel" data-dismiss="modal"
                                                            data-bind="click: function(data, event) { cancelForgotPasswordSection.bind(data, $parent, event)() }, widgetLocaleText: 'buttonCancel', event: {mousedown: handleMouseDown.bind($data, $parent), mouseup: handleMouseUp.bind($data, $parent)}"></button>
